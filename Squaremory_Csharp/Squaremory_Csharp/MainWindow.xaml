<Window x:Class="Squaremory_Csharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Squaremory_Csharp"
        mc:Ignorable="d"
        Title="Squaremory" Height="450" Width="800">
    <Grid>
        <Grid HorizontalAlignment="Left" Height="354" VerticalAlignment="Top" Width="794" ShowGridLines="True" Margin="0,66,-0.4,0" Background="Black" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
        </Grid>
        <Button x:Name="startbtn" Content="Start" HorizontalAlignment="Left" Height="51" Margin="140,10,0,0" VerticalAlignment="Top" Width="92" FontSize="20"/>
        <Label x:Name="timelbl" Content="Label" HorizontalAlignment="Left" Height="51" Margin="237,10,0,0" VerticalAlignment="Top" Width="78"/>

    </Grid>
</Window>
